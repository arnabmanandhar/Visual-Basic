Module Module1

    Sub Main()
        Dim num, I As Integer
        Console.Write("Input a number : ")
        num = Console.ReadLine
        Console.WriteLine("Table of " & num)
        Console.WriteLine("- - - - - - - - -")
        For I = 1 To 10
            Console.WriteLine("{0}  x   {1}  =  {2}", I, num, I * num)
        Next I
        Console.Write("- - - - - - - - -")
        Console.ReadKey()
    End Sub

End Module
